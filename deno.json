{
  "tasks": {
    "dev": "deno run -A --watch=static/,routes/,islands/,components/,lib/ dev.ts",
    "build": "deno run -A dev.ts build",
    "preview": "deno run -A main.ts"
  },
  "imports": {
    "@fresh/core": "jsr:@fresh/core@^2.2.0",
    "@fresh/core/dev": "jsr:@fresh/core@^2.2.0/dev",
    "@preact/signals": "npm:@preact/signals@^1.3.1",
    "preact": "npm:preact@^10.24.3",
    "preact/hooks": "npm:preact@^10.24.3/hooks"
  },
  "compilerOptions": {
    "lib": ["dom", "dom.asynciterable", "deno.ns"],
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  }
}
